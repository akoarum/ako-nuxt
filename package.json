  "scripts": {
    "dev": "cross-env NODE_ENV=\"dev\" nuxt",
    "dev:local": "cross-env NODE_ENV=\"local\" nuxt",
    "generate:dev": "cross-env NODE_ENV=\"dev\" nuxt generate",
    "generate:staging": "cross-env NODE_ENV=\"staging\" nuxt generate",
    "generate:prod": "cross-env NODE_ENV=\"prod\" nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "precommit": "npm run lint",
    "test": "vue-cli-service test:unit --config ./jest.config.js",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "hygen": "hygen component new"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "nuxt": "^2.5.1",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
