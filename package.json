    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-preset-vue": "^2.0.2",
    "bluebird": "^3.5.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
